{
  "name": "slack-api-query-bot",
  "version": "1.0.0",
  "description": "A Slack bot that processes natural language queries and calls APIs",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=production node server-combined.js",
    "dev": "NODE_ENV=development nodemon server-combined.js",
    "start:legacy-socket": "USE_SOCKET_MODE=true node app.js",
    "start:legacy-express": "node server.js",
    "dev:legacy": "nodemon app.js",
    "test": "jest",
    "test-auth": "node test/dynamic-auth-test.js",
    "test-email": "node test/email-priority-test.js",
    "test-accounts": "node test/real-account-ids-test.js",
    "test-payload": "node test/api-payload-test.js",
    "test-storage": "node test/real-connection-storage-test.js",
    "demo-auth": "node scripts/test-dynamic-auth.js",
    "health-check": "curl -f http://localhost:3000/health || exit 1"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@pipedream/sdk": "^1.7.0",
    "@slack/bolt": "^3.17.1",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "mongoose": "^8.16.5",
    "openai": "^5.10.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "slack",
    "bot",
    "api",
    "natural-language"
  ],
  "author": "Your Name",
  "license": "MIT"
}
